  </line>
  <line no="18">
    <token name="T_DOC_COMMENT"> *</token>
  </line>
  <line no="19">
    <token name="T_DOC_COMMENT"> * @method HasManyList Codes() List of PermissionRoleCode objects</token>
  </line>
  <line no="20">
    <token name="T_DOC_COMMENT"> * @method ManyManyList Groups() List of Group objects</token>
